{
  "type": "module",
  "name": "fastify",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "pnpm run build:ts && tsc -p test/tsconfig.json && FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --test --experimental-test-coverage --loader ts-node/esm test/**/*.ts",
    "vitest": "FASTIFY_AUTOLOAD_TYPESCRIPT=1 vitest",
    "start": "pnpm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "pnpm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "joist-codegen": "pnpx tsx ./node_modules/joist-codegen",
    "joist-migrate": "pnpx tsx ./node_modules/joist-migration-utils",
    "joist-new-migration": "pnpx node-pg-migrate create"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.4.2",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0",
    "joist-orm": "^1.235.0",
    "knex": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^3.0.0",
    "joist-codegen": "^1.235.0",
    "joist-migration-utils": "^1.235.0",
    "joist-utils": "^1.235.0",
    "node-pg-migrate": "^7.9.1",
    "ts-node": "^10.4.0",
    "typescript": "~5.8.2",
    "vitest": "^3.1.1"
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971"
}
